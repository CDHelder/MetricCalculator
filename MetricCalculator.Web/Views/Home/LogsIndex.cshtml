@model IEnumerable<Calculation>

@{
    ViewData["Title"] = "LogsIndex";
}

<h1>Logs Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateTimeCalculated)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BeforeValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AfterValue)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateTimeCalculated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BeforeValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AfterValue)
            </td>
            @*<td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>*@
        </tr>
}
    </tbody>
</table>
